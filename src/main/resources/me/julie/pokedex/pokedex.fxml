<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.julie.pokedex.PokedexController">
    <AnchorPane prefHeight="600.0" prefWidth="900.0">
      <children>
         <VBox fx:id="mainVBox" alignment="TOP_CENTER" prefHeight="600.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
               <Label fx:id="starterLabel" text="Starter Pokemon">
                  <font>
                     <Font name="Candara Italic" size="40.0" />
                  </font>
               </Label>
               <Label fx:id="pokedexLabel" text="Pokedex">
                  <font>
                     <Font name="Candara Bold Italic" size="96.0" />
                  </font>
               </Label>
            </children>
            <padding>
               <Insets top="30.0" />
            </padding>
         </VBox>
      </children></AnchorPane>
</Scene>
